{
  "name": "smart-clicks",
  "displayName": "Smart Clicks",
  "version": "0.0.11",
  "publisher": "antfu",
  "packageManager": "pnpm@6.32.3",
  "description": "Smart selection with double clicks",
  "homepage": "https://github.com/antfu/vscode-smart-clicks#readme",
  "bugs": {
    "url": "https://github.com/antfu/vscode-smart-clicks/issues"
  },
  "license": "MIT",
  "author": "Anthony Fu <anthonyfu117@hotmail.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/antfu/vscode-smart-clicks"
  },
  "funding": "https://github.com/sponsors/antfu",
  "main": "./dist/index.js",
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "engines": {
    "vscode": "^1.65.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "*"
  ],
  "icon": "res/icon.png",
  "contributes": {
    "configuration": {
      "properties": {
        "smartClicks.rules": {
          "type": "object",
          "properties": {
            "bracket-pair": {
              "type": "boolean",
              "default": true,
              "description": "Pair to inner content of brackets.\n\n```js\n▽\n(foo, bar)\n └──────┘\n```"
            },
            "dash": {
              "type": "boolean",
              "default": true,
              "description": "`-` to identifier.\n\n```css\n   ▽\nfoo-bar\n└─────┘\n```"
            },
            "html-attr": {
              "type": "boolean",
              "default": true,
              "description": "`=` to HTML attribute.\n\n```html\n          ▽\n<div class=\"btn\"></div>\n     └─────────┘\n```"
            },
            "html-element": {
              "type": "boolean",
              "default": true,
              "description": "`<` to the entire element.\n\n```html\n▽\n<div><div></div></div>\n└────────────────────┘\n```"
            },
            "html-tag-pair": {
              "type": "boolean",
              "default": true,
              "description": "Open and close tags of a HTML element.\n\n```html\n ▽\n<div><div></div></div>\n └─┘              └─┘\n```"
            },
            "js-arrow-fn": {
              "type": "boolean",
              "default": true,
              "description": "`=>` to arrow function.\n\n```js\n       ▽\n(a, b) => a + b\n└─────────────┘\n```"
            },
            "js-assign": {
              "type": "boolean",
              "default": true,
              "description": "`=` to assignment.\n\n```js\n        ▽\nconst a = []\n└──────────┘\n```"
            },
            "js-block": {
              "type": "boolean",
              "default": true,
              "description": "Blocks like `if`, `for`, `while`, etc. in JavaScript.\n\n```js\n▽\nfunction () {     }\n└─────────────────┘\n```\n\n```js\n▽\nimport { ref } from 'vue'\n└───────────────────────┘\n```"
            },
            "js-colon": {
              "type": "boolean",
              "default": true,
              "description": "`:` to the value.\n\n```js\n     ▽\n{ foo: { bar } }\n       └─────┘\n```"
            },
            "jsx-tag-pair": {
              "type": "boolean",
              "default": true,
              "description": "Matches JSX elements' start and end tags.\n\n```jsx\n  ▽\n(<Flex.Item>Hi</Flex.Item>)\n  └───────┘     └───────┘\n```"
            }
          },
          "description": "Rule toggles"
        }
      }
    }
  },
  "scripts": {
    "build": "NODE_ENV=production tsup",
    "dev": "NODE_ENV=development tsup --watch",
    "lint": "eslint .",
    "vscode:prepublish": "nr build",
    "publish": "vsce publish --no-dependencies --allow-star-activation",
    "pack": "vsce package --no-dependencies --allow-star-activation",
    "test": "vitest",
    "typecheck": "tsc --noEmit",
    "release": "bumpp --commit --push --tag && nr publish",
    "readme": "esno ./scripts/docs.ts"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.18.8",
    "@antfu/ni": "^0.13.2",
    "@antfu/utils": "^0.5.0",
    "@babel/core": "^7.17.8",
    "@babel/parser": "^7.17.8",
    "@babel/traverse": "^7.17.3",
    "@babel/types": "^7.17.0",
    "@types/node": "^17.0.21",
    "@types/vscode": "^1.65.0",
    "bumpp": "^7.1.1",
    "eslint": "^8.11.0",
    "esno": "^0.14.1",
    "fast-glob": "^3.2.11",
    "htmlparser2": "^7.2.0",
    "node-html-parser": "^5.3.3",
    "pnpm": "^6.32.3",
    "rimraf": "^3.0.2",
    "tsup": "^5.12.1",
    "typescript": "^4.6.2",
    "vite": "^2.8.6",
    "vitest": "^0.7.4",
    "vsce": "^2.7.0"
  }
}
